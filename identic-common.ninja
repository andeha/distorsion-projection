#  identic-common.ninja - for the libTown.a math library.
commandline-sdkpath = /Library/Developer/CommandLineTools/SDKs/MacOSX.sdk
xcodeapp-sdkpath = /Applications/Xcode.app/Contents/Developer/Platforms/MacOS.sdk
moduleflags = -fmodules-ts -fimplicit-modules -fmodule-map-file={$MAPFILE}
macosflags = -isysroot $commandline-sdkpath
gencdflags = -fblocks -fno-rtti -fno-coroutines-ts -fno-exceptions          $
 -fno-builtin
compiler-macos = xcrun clang
compiler-pic32 = {$TOOLS_SMALL}/clang-12
